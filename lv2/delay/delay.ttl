@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .

<https://github.com/ganajtur0/pluginok/delay>
	a lv2:Plugin , lv2:AmplifierPlugin , doap:Project ;
	lv2:binary <delay.so> ;
	rdfs:seeAlso <delay.ttl> ;
	doap:name "Delay" ,
		  "Késleltetés"@hu;
	lv2:optionalFeature lv2:hardRTCapable ;
	lv2:port
		[
			a lv2:InputPort , lv2:AudioPort ;
			lv2:index 0 ;
			lv2:symbol "audio_in" ;
			lv2:name "Audio Input" ,
				 "Hangbemenet"@hu ;
		] ,
		[
			a lv2:OutputPort , lv2:AudioPort ;
			lv2:index 1 ;
			lv2:symbol "audio_out" ;
			lv2:name "Audio Output" ,
				 "Hangkimenet"@hu ;
		] ,
		[
			a lv2:InputPort , lv2:ControlPort ;
			lv2:index 2 ;
			lv2:symbol "time" ;
			lv2:name "Delay Time" , 
				 "Késleltetés"@hu ;
			lv2:minimum 0.0;
			lv2:maximum 2.0;
			lv2:default 0.1;
		] ,
		[
			a lv2:InputPort , lv2:ControlPort ;
			lv2:index 3 ;
			lv2:symbol "feedback" ;
			lv2:name "Feedback" ,
				 "Visszacsatolás"@hu ;
			lv2:minimum 0.0 ;
			lv2:maximum 0.9 ;
			lv2:default 0.7 ;
		] ,
		[
			a lv2:InputPort , lv2:ControlPort ;
			lv2:index 4 ;
			lv2:symbol "mixDry" ;
			lv2:name "Dry Mix" ,
				 "Száraz jel"@hu ;
			lv2:minimum 0.0 ;
			lv2:maximum 1.0 ;
			lv2:default 1.0 ;
		] ,
		[
			a lv2:InputPort , lv2:ControlPort ;
			lv2:index 5 ;
			lv2:symbol "mixWet" ;
			lv2:name "Wet Mix" ,
				 "Nedves jel"@hu ;
			lv2:minimum 0.0 ;
			lv2:maximum 1.0 ;
			lv2:default 1.0 ;
		] .
